    "build_win": "bun build --compile --bun-windows-x64 ./index.ts  --outfile dist/receiver-server"
  },
  "type": "module",
  "dependencies": {
    "bunrest": "^1.3.8",
    "ip": "^2.0.1",
    "multer": "^1.4.5-lts.1"
  }
